#include <stdio.h>

void fonk1(int d[], int N)
 {
    int i;
    for (i = 0 ; i < N ; i++)
    printf("%d\n", d[i]);
    printf("\n");
}
void fonk2(int *d, int N)
{
    int i;

    for (i = 0 ; i < N ; i++)
    printf("%d\n", d[i]);
    printf("\n");
}
int main()
{
    int arr[7] = {0, 10, 20, 30, 40, 50, 60};

    fonk1(arr, 2);
    fonk2(arr, 2);
    fonk1(&arr[2], 3);
    fonk1(arr+2, 3);
return 0;
}

#include <stdio.h>

void ekrana_yaz(int d[], int N)
 {
    int i;
    for (i = 0 ; i < N ; i++)
    printf("%d\n", d[i]);
    printf("\n");
}
void ekrana_yaz_2(int *d, int N) 
{
    int i;
    
    for (i = 0 ; i < N ; i++)
    printf("%d\n", d[i]);
    printf("\n");
}
int main() 
{
    int dizi[7] = {0, 10, 20, 30, 40, 50, 60};
    
    ekrana_yaz(dizi, 2);
    ekrana_yaz_2(dizi, 2);
    ekrana_yaz(&dizi[2], 3);
    ekrana_yaz(dizi+2, 3);
return 0;
}
